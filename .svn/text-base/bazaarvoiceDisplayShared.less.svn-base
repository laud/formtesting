                .BVDI_PSBodyCrossProduct {}
                    .BVDI_PSSectionContributions {}
                        .BVDI_PSHeaderContributions {.less_text_strong; .less_text_size4;}
                        .BVDI_PSBodyContributions {.less_text_size6; .less_text_lowercase; float: left; margin-top: 5px;}
                            .BVDI_PSBodyContributions .BVDI_PSLabelValuePair {display: inline;}
                                .BVDI_PSNumberContributions {.less_text_strong; display: inline;}
            .BVDI_PSContainerProductSpecific {width: 45%; margin-right: 5%; float: left;}
                .BVDI_PSHeaderProductSpecific {}
                .BVDI_PSBodyProductSpecific {.less_text_meta;}
                .BVDI_PSBodyProductSpecificPRR {}
                    .BVDI_PSSectionAverageRating {overflow: hidden; zoom: 1; margin-bottom: 10px; padding: 2px 0;}
