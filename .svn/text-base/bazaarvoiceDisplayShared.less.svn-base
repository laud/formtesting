/* Badges 2.0 (BVDI_BA*) */
.BVDI_BA {.less_text_body;}
    .BVDI_BAHeader {}
    .BVDI_BABody {}
        .BVDI_BAList {margin: 5px 0 0; padding: 0; list-style-type: none;}
            .BVDI_BAItem {margin: 0 5px 0 0; display: inline-block; *display: inline;} /* HACK: IE6 IE7 [dbeberniss] display: inline; property necessary due to incomplete support of display: inline-block; - used to create single row of badges */

/* ===== Badge Sprite Guide =====
     0  - Miscellaneous
   - 24 - Contributor
   - 48 - Top Contributor
   - 72 - Expert Contributor
   - 96 - Staff Contributor
   -120 - Best Content
   -144 - Featured Content
*/

/* ===== Vertical Alignment Hack =====
   Browsers calculate vertical alignment differently, so a combination of height, line-height, and top padding was used to get all text to appear to be vertical-align: middle in relation to the badge image.
   .BVDI_BAItem .BVDILink {height: 24px;}
     .BVDI_BAItem .BVDILinkSpan {line-height: 20px; padding-top: 4px; vertical-align: top;}
*/
            .BVDI_BAItem .BVDILink {text-transform: uppercase; height: 16px; display: inline-block; overflow: hidden; cursor: pointer;}
            .BVDI_BAItem .BVDILink {.less_text_linkAction;}
            .BVDI_BAItem .BVDILink:visited {.less_text_linkActionVisited;}
            .BVDI_BAItem .BVDILink:hover {.less_text_linkActionHover;}
            .BVDI_BAItem .BVDILink:focus {.less_text_linkActionFocus;}
            .BVDI_BAItem .BVDILink:active {.less_text_linkActionActive;}
            .BVDI_BAItem .BVDILink {background: url(iconBadge.gif) no-repeat scroll 0 0 transparent;}
                .BVDI_BAItem .BVDILink:hover {}
                .BVDI_BAItem .BVDILink:active {}
                .BVDI_BAItem .BVDILinkDisabled:hover {}
            .BVDI_BAItemContributor .BVDILink {background-position: 0 -80px; padding-left: 20px;}
                .BVDI_BAItemContributor .BVDILink:hover {}
                .BVDI_BAItemContributor .BVDILink:active {}
                .BVDI_BAItemContributor .BVDILinkDisabled:hover {}
            .BVDI_BAItemTop1Contributor .BVDILink {background-position: 0 0; padding-left: 20px;}
                .BVDI_BAItemTop1Contributor .BVDILink:hover {}
                .BVDI_BAItemTop1Contributor .BVDILink:active {}
                .BVDI_BAItemTop1Contributor .BVDILinkDisabled:hover {}
            .BVDI_BAItemExpert .BVDILink {background-position: 0 -120px; padding-left: 20px;}
                .BVDI_BAItemExpert .BVDILink:hover {}
                .BVDI_BAItemExpert .BVDILink:active {}
                .BVDI_BAItemExpert .BVDILinkDisabled:hover {}
            .BVDI_BAItemStaff .BVDILink {background-position: 0 -37px; padding-left: 20px;}
                .BVDI_BAItemStaff .BVDILink:hover {}
                .BVDI_BAItemStaff .BVDILink:active {}
                .BVDI_BAItemStaff .BVDILinkDisabled:hover {}
            .BVDI_BAItemBuckleStoreLeader .BVDILink {background-position: 0 -37px; padding-left: 20px;}
            .BVDI_BAItemMerchandiseBuyer .BVDILink {background-position: 0 -37px; padding-left: 20px;}
            .BVDI_BAItemBuckleStylist .BVDILink {background-position: 0 -37px; padding-left: 20px;}
            .BVDI_BAItemBrandRepresentative .BVDILink {background-position: 0 -120px; padding-left: 20px;}
            .BVDI_BAItemBuckleTeammate .BVDILink {background-position: 0 -37px; padding-left: 20px;}
            .BVDI_BAItemBest .BVDILink {}
