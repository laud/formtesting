.BVDI_PSPopin, #BVDI_PSIEControlsFrame {position: absolute; z-index: 10000000; margin: 7px; overflow: hidden; zoom: 1;} /* Note: margin-left/right are preserved when popin is dynamically repositioned */
.BVDI_PS {.less_text_body; .less_box_popin; line-height: 1.5em; width: 450px;}
    .BVDI_PS a {.less_text_link;}
    .BVDI_PS a:visited {.less_text_linkVisited;}
    .BVDI_PS a:hover {.less_text_linkHover;}
    .BVDI_PS a:focus {.less_text_linkFocus;}
    .BVDI_PS a:active {.less_text_linkActive;}
    .BVDI_PSInside {padding: 15px;}
        .BVDI_PSHeader {float: right;}
            .BVDI_PSHeader .BVDILinkClose {display: none;}
        .BVDI_PSBody {margin-bottom: 10px; clear: both;}
            /* Styles common to all sections */
            .BVDI_PSContainer {margin-bottom: 15px;}
                .BVDI_PSHeaderContainer{}
                .BVDI_PSBodyContainer {overflow: hidden; zoom: 1;}
                    .BVDI_PSSection {}
                        .BVDI_PSHeaderSection {}
                        .BVDI_PSBodySection {}
            /* Styles specific to a particular section */
            .BVDI_PSContainerBasic {overflow: hidden; zoom: 1;}
                .BVDI_PSAvatar {float: left; width: 45px; margin-right: 10px;}
                .BVDI_PSAvatar a, .BVDI_PSAvatar img {display: block; float: left; width: 100%; height: auto;}
                .BVDI_PSUserLocationContainer, .BVDI_PSContextDataContainer {clear: both;}
                .BVDI_PSUserLocationContainer .BVDI_PSLabel, .BVDI_PSContextDataContainer .BVDI_PSLabel {.less_text_strong;}
                .BVDI_PSHeaderBasic {}
                .BVDI_PSBodyBasic {}
                    .BVDI_PSSectionNickname {.less_text_nickname; .less_text_strong; float: left;}
                        .BVDI_PS .BVDI_PSSectionNickname a {padding-top: 10px;}
                    .BVDI_PSBodyBasic .BVDI_PSSectionBadges {float: right;}
                        .BVDI_PSBodyBasic .BVDI_BABadges {margin-bottom: 5px;}
                    .BVDI_PSSectionBadges .BVDI_BAItem .BVDILinkSpan {}
