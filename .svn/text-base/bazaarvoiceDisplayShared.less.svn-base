                        .BVDI_PSSectionCrossLinkingPRR .BVDI_PSRatingOutOf {display: none;}
                    .BVDI_PSHeaderCrossLinkingPRR {.less_text_strong; margin-bottom: 10px;}
                .BVDI_PSBodyCrossLinkingQA {clear: left;}
                    .BVDI_PSSectionCrossLinkingQA {}
                    .BVDI_PSHeaderCrossLinkingQA {.less_text_strong; margin-bottom: 10px;}
