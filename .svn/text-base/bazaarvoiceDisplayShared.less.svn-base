/* ******** DISPLAY (BVDI) ******** */

/* Button Styles. */
/* Change me to match the client sites! Be sure to match these styles in the other CSS files also! */
/* If you want progressive enhancements, find them in progressive */

    /* General Button Style */
    .BVSUButton {line-height: 1.5em; margin: 0 10px 0 0; vertical-align: top; display: inline-block; *display: inline; padding: 3px 10px; *padding: 0;} /* HACK: IE6 IE7 [dbeberniss] Reduce padding on all sides because these browsers all additional padding to buttons */
    .BVSUButton {.less_box_btnLow; .less_text_btnLow;}
    .BVSUButton:hover {.less_box_btnLowHover; .less_text_btnLowHover;}

    /* Report as Inappropriate submission */
    #BVSUSubmitReviewInappropriateButtonID, #BVSUSubmitReviewCommentInappropriateButtonID {.less_box_btnHigh; .less_text_btnHigh;}
    #BVSUSubmitReviewInappropriateButtonID:hover, #BVSUSubmitReviewCommentInappropriateButtonID:hover {.less_box_btnHighHover; .less_text_btnHighHover;}

    /* Content Subscription */
    .BVContentSubscription .BVFieldButton {margin-bottom: 0;}
    .BVContentSubscription button {.less_box_btnLow; .less_text_btnLow; margin: 0 10px 0 0; padding: 3px 10px; display: inline-block; vertical-align: top; cursor: pointer; *display: inline; *padding: 0;} /* HACK: IE6 IE7 [dbeberniss] display: inline; necessary due to incomplete support of inline-block, reduce padding on all sides because these browsers all additional padding to buttons */
    .BVContentSubscription button:hover {.less_box_btnLowHover; .less_text_btnLowHover;}
    .BVContentSubscription .BVFieldSubmit button, .BVPageContentSubscription .BVSUModuleButtons #BVSUSubmitSubscriptionButtonID {.less_box_btnHigh; .less_text_btnHigh;}
    .BVContentSubscription .BVFieldSubmit button:hover, .BVPageContentSubscription .BVSUModuleButtons #BVSUSubmitSubscriptionButtonID:hover {.less_box_btnHighHover; .less_text_btnHighHover;}


/* General */
.BVDIBody, .BVVIVideoPopup {.less_text_body;}
.BVDILinkDisabled {cursor: default;}
.BVDIPopup {position: absolute;}
.BVDIHidden {display: none;}
.BVDISeparator, .BVDISpacer {height: 0; line-height: 0; overflow: hidden;}
.BVDIOverlay, .BVDIPopin {position: absolute;}
.BVDILinkClose {.less_text_iconClose; .less_text_iconOnly; float: right;}
.BVDILinkClose {.less_text_linkAction;}
.BVDILinkClose:visited {.less_text_linkActionVisited;}
.BVDILinkClose:hover {.less_text_linkActionHover;}
.BVDILinkClose:focus {.less_text_linkActionFocus;}
.BVDILinkClose:active {.less_text_linkActionActive;}
.BVDINickname {.less_text_strong;}

/* Container pages that don't inherit styles from the product css */
.BVDIPopIn {.less_text_size5; .less_text_fontFamily; color: @less_text_colorNormal; line-height: 1.5em;}
.BVDIPopIn a {.less_text_link;}
.BVDIPopIn a:visited {.less_text_linkVisited;}
.BVDIPopIn a:hover {.less_text_linkHover;}
.BVDIPopIn a:focus {.less_text_linkFocus;}
.BVDIPopIn a:active {.less_text_linkActive;}

/* Content search (BVDI_SE) */
.BVDI_SEForm {float: right; margin: -1px 0 0 15px;}
    .BVDI_SEFormTextInputLabel {font-weight: bold;}
.BVDI_SEResultIndex {float: left; padding: 15px 0 0 15px; width: 50px; font-size: 18pt;}

/* Product Linking Display (BVDI_PL) */
.BVDI_PL {.less_box_meta; .less_text_meta; margin-top: 10px; padding: 10px; zoom: 1;}
    .BVDI_PLSeparatorTop {}
    .BVDI_PLHeader {margin-bottom: 10px;}
        .BVDI_PLTitle {.less_text_titleHelper;}
    .BVDI_PLBody {}
        .BVDI_PLHelperText {}
        .BVDI_PLProductList {}
            .BVDI_PLProduct {padding: 10px; margin-bottom: 10px; overflow: hidden; zoom: 1;}
                .BVDI_PLSpacerProduct {display: none;}
                .BVDI_PLSpacerProduct2, .BVDI_PLSpacerProduct5, .BVDI_PLSpacerProduct8 {display: block; clear: left;} /* 3 columns */
                .BVDI_PLInside {}
                    .BVDI_PLProductPrimary {}
                        .BVDI_PLProductImage {width: 50px; height: 50px; overflow: hidden; float: left; margin-right: 10px;}
                        .BVDI_PLProductImage a {.less_box_opacity70;}
                        .BVDI_PLProductImage a:hover {.less_box_opacity100;}
                        .BVDI_PLProductImage img {width: 100px; margin-left: -25px; -ms-interpolation-mode: bicubic; zoom: 1;}
                        .BVDI_PLProductName {}
                        .BVDI_PLProductRatings {padding-top: 2px;}
                            .BVDI_PLProductRatings img {width: 85px;}
                            .BVDI_PLProductRatings a {float: left;}
                            .BVDI_PLProductRatings a.BVDILinkProductRatingText, .BVDI_PLProductRatings .BVDILinkSpan {padding-left: 3px; float: left;}
                            .BVDI_PLProductRatings .BVDILinkProductRatingText {display: none;}
                            .BVDI_PLProductRatings .BVRRCount {color: @less_text_colorLow;}
                        .BVDI_PLSpacerProductPrimary {}
                    .BVDI_PLSpacerInside {}
        .BVDI_PLAdditionalText {display: none;} /* This container is rendered even when it's empty, causing extra padding in IE. When Additional text is turned on, remove this style. */
        .BVDI_PLSpacerBody {clear: left;}
    .BVDI_PLSeparatorBottom {}


    /* BEGIN: QuickTake, Attribute Filtering Display Style 1 (BVDI_QT, BVDI_AF) */
    /* NOTE: Used by R&R */
    .BVDI_QT {height: auto; -moz-box-sizing: padding-box; z-index: 0;}
    .BVDI_QT .BVRRSecondaryRatingSummary {float: none; width: auto;} /* Overrides a float necessary for QT1 */
        .BVDI_QTHeader {margin: 5px 0 10px;}
            .BVDI_QTTitle {margin-bottom: 5px;}
            .BVDI_QTSubtitle {.less_text_helper;}
        .BVDI_QTBody {height: 60px;}
        .BVDI_AFAttributeFilteringSectionCell {/*see bazaarvoice for structural elements*/}
        .BVDI_AF {clear: both;}
             .BVDI_AFHeader {.less_text_titleHelper; color: @less_text_colorNormal; display: block; padding: 0 0 8px;}
            .BVDI_AFBody {overflow: hidden;}
            .BVDI_AFHidden {display: none;}
            .BVDI_AFFilterAttribute {width: auto; float: left; cursor: pointer; z-index: 0; margin: 0 10px 5px 0; zoom: 1;}

            /* Filter links */
            .BVDI_AFFilterAttributeHeader {.less_text_checkFilterOff; line-height: 1.2em;}
            .BVDI_AFFilterAttributeHeader:hover {.less_text_checkFilterOffHover;}
            .BVDI_AFFilterAttributeSelected .BVDI_AFFilterAttributeHeader {.less_text_checkFilterOffHover;}
            .BVDI_AFFilterAttributeExpanded .BVDI_AFFilterAttributeHeader {.less_text_checkFilterOffHover;}
            .BVDI_AFFilterAttributeSelected .BVDI_AFFilterAttributeHeader:hover {.less_text_checkFilterOnHover;}
            .BVDI_AFFilterAttributeDisabled .BVDI_AFFilterAttributeHeader, .BVDI_AFFilterAttributeDisabled .BVDI_AFFilterAttributeHeader:hover {.less_text_checkFilterDisabled;}
            .BVDI_AFAttributeFilteringBeforeSpacer {margin-top: 5px;}
            .BVDI_AFFilterAttributeContent, #BVRRFilterAttributeContentIEControlsFrameID, #BVDI_QTToggleAreaGroupIEControlsFrameID {background-color: @less_box_colorBase; position: absolute; min-width: 115px; margin-left: -1px; *width: 115px; *overflow: visible;} /* HACK: IE6 IE7 [dbeberniss] Set fixed width because IE6 IE7 don't respect the min-width property. Keep filter dropdowns and QT iframes that go behind them the same width. */
            .BVDI_AFFilterAttributeContent {.less_box_popin; cursor: auto; z-index: 100;}
            #BVRRFilterAttributeContentIEControlsFrameID {z-index: 50; overflow: hidden;}
            .BVDI_AFFilterAttributeContentTitle {.less_text_helper; .less_text_emphasis; margin-left: 5px; margin-right: 5px; line-height: 14px; max-width: 100px;}
            .BVDI_AFFilterAttributeContent ul, .BVDI_AFFilterAttributeContent li {padding: 0; margin: 6px 4px !important; list-style-type: none !important;}
                .BVDI_AFFilterAttributeContent ul {overflow: hidden; position: relative;}
                .BVDI_AFFilterAttributeContent li input {position: absolute; left: -100px;}
                .BVDI_AFFilterAttributeContent .BVDI_AFFilterAttributeLabel a {.less_text_checkFilterOff;}
                .BVDI_AFFilterAttributeContent .BVDI_AFFilterAttributeLabel a:hover, .BVDI_AFFilterAttributeContent .BVDI_AFFilterAttributeLabel a:focus, .BVDI_AFFilterAttributeContent .BVDI_AFFilterAttributeLabel a:active {.less_text_checkFilterOffHover;}
                .BVDI_AFSelected .BVDI_AFFilterAttributeLabel a {.less_text_checkFilterOn;}
                .BVDI_AFSelected .BVDI_AFFilterAttributeLabel a:hover, .BVDI_AFSelected .BVDI_AFFilterAttributeLabel a:focus, .BVDI_AFSelected .BVDI_AFFilterAttributeLabel a:active {.less_text_checkFilterOnHover;}
                .BVDI_AFFilterAttributeContent .BVDI_AFFilterAttributeLabelDisabled {.less_text_checkFilterDisabled;}
                .BVDI_AFFilterAttributeContent .BVDI_AFFilterAttributeLabel {}
                .BVDI_AFFilterAttributeContent .BVDI_AFFilterAttributeLabel a {.less_text_linkAction; .less_text_strong;}
                .BVDI_AFFilterAttributeLabelDisabled {color: @less_text_colorLow;}
            .BVDI_AFAttributeCount {color: @less_text_colorLow;}
            .BVDI_AFAttributeFilterItemOn, .BVRRAttributeFilterItemOn {}
            .BVDI_AFAttributeFilterItemOn a, .BVRRAttributeFilterItemOn a {.less_text_checkFilterOn;}
            .BVDI_AFAttributeFilterItemOn a:hover, .BVRRAttributeFilterItemOn a:hover {.less_text_checkFilterOnHover;}
            .BVDI_AFFilterAttributeClose {display: none;}
            .BVDI_AFAttributeFilteringShowMore {text-align: right; padding-right: 5px; padding-bottom: 5px;}
    /* END: QuickTake, Attribute Filtering Display Style 1 (BVDI_QT, BVDI_AF) */

    /* QT Histogram */
         .BVDI_QTHistogram {display: block; width: auto;}
         .BVDI_QTAttributes .BVDI_QTHistogram {margin-bottom: 7px;}
            .BVDI_QTToggles {float: right;}
             .BVDI_QTGroupWithHistogram .BVDIToggleArea {width: 180px;}
             .BVDI_QTHistogramBar, .BVDI_QTHistogramPercent {.less_text_size6; color: @less_text_colorLow; display: inline-block; padding-top: 1px;}
             .BVDI_QTHistogramPercent {vertical-align: top;}
             .BVDI_QTTags li.BVDI_QTFilter {margin-bottom: 8px;}
             .BVDI_QTHeaderGroup .BVDINumber {.less_text_normal; color: @less_text_colorLow; .less_text_size6;}
             #BVRRQTFilterHeaderToggleShowHistogramsID {display: none;}
             .BVDI_QTGroupWithHistogram {}
                 .BVDI_QTHistogram {display: inline-block;}
                 .BVDI_QTGroupWithHistogram .BVDI_QTFilter .BVDINumber {display: none;}
                 .BVDI_QTGroupWithHistogram .BVDI_QTFilter .BVDIValue {margin-right: 5px; white-space: normal;}
                     /* Visual Histograms - uncomment if you want to use the visual representation of the histograms */
                     /* .BVDI_QTHistogramBar {.less_box_borderLight; width: 55px; height: 10px; margin-right: 5px; font-size: 0; vertical-align: top;}
                        .BVDI_QTHistogramBarFull {display: block; height: 10px; display: inline-block; margin-right: 5px; font-size: 0; line-height: 0; vertical-align: top;}
                        .BVDI_QTGroupWithHistogram .BVDI_QTFilter .BVDIValue {display: inline-block; margin-right: 5px; vertical-align: top; width: 92px; white-space: normal;} */
                /* No histograms */
                .BVDI_QTHideHistograms .BVDI_QTHistogram, .BVDI_QTHideHistograms #BVRRQTFilterHeaderToggleHideHistogramsID {display: none;}
                .BVDI_QTHideHistograms #BVRRQTFilterHeaderToggleShowHistogramsID {display: block;}
                /* Filters */
                .BVDI_QTGroupWithHistogram .BVDI_QTPopularLabel {display: block; vertical-align: top;}
                .BVDI_QTAttributes .BVDI_QTGroupWithHistogram {vertical-align: top; display: inline-block;}
                .BVDI_QTAttributes .BVDI_QTHistogram {width: 110px;}
                .BVDIToggleArea .BVDI_QTHistogram {width: auto;}
                .BVDI_QT .BVDI_QTGroupWithHistogram a span .BVDINumber {color: @less_text_colorLow; .less_text_size6;}
                .BVDI_QT .BVDI_QTGroupWithHistogram a span, .BVDI_QT .BVDI_QTBodyAttributes a:hover {white-space: normal;}
                .BVRRContainer .BVDI_QTHeaderAttributes, .BVRRContainer .BVDI_QTBodyAttributes {margin-bottom: 0 !important;}
                .BVDI_QT .BVDI_QTHeaderAttributes .BVDITitle {.less_text_size5;}


/* ******** QuickTake Display Style 2 DISPLAY (BVDI) ******** */
/* QuickTake, Attribute Filtering, Display Style 2 (BVDI_QT, BVDI_AF) */
/* General QuickTake styles used by filtering in the dashboard, AND inline with each piece of content */
        /* BVDI_QTGroups */
        .BVDI_QTGroup {float: left; margin: 0 10px 5px 0;}
            .BVDI_QTGroup a.BVDILinkToggle {line-height: 1.2 !important}

        .BVDI_QTGroupOdd {}
        .BVDI_QTGroupEven {}
        .BVDI_QTGroupFirst {}
        .BVDI_QTGroupLast {}
        .BVDI_QTGroupNoContent .BVDI_QTHeaderGroup .BVDITitle {.less_text_normal; .less_text_meta; .less_text_emphasis;}
        .BVDI_QTGroupNoContent .BVDI_QTHeaderGroup .BVDILink, .BVDI_QTGroupNoContent .BVDI_QTHeaderGroup .BVDILink:visited, .BVDI_QTGroupNoContent .BVDI_QTHeaderGroup .BVDILink:hover, .BVDI_QTGroupNoContent .BVDI_QTHeaderGroup .BVDILink:focus, .BVDI_QTGroupNoContent .BVDI_QTHeaderGroup .BVDILink:active {.less_text_linkAction; .less_text_normal; .less_text_meta; .less_text_emphasis;}
            .BVDI_QTInsideGroup {}
                .BVDI_QTHeaderGroup {}
                    .BVDI_QTHeaderGroup .BVDITitle {}
        .BVDI_QTTags .BVDI_QTGroup {}
        .BVDI_QTAttributes .BVDI_QTGroup {padding: 3px 0; margin-right: 10px; z-index: 2;}
        .BVDI_QTGroupExpanded {}
            .BVDI_QTAttributes .BVDI_QTGroupExpanded .BVDITitle .BVDILinkToggle {}
        .BVDI_QTGroupCollapsed {}
            .BVDI_QTAttributes .BVDI_QTGroupCollapsed .BVDITitle .BVDILinkToggle {line-height: 1.2;}
            .BVDI_QTInsideGroup {}
            .BVDI_QTHeaderGroup {}
                .BVDI_QTPrefixHeaderGroup {display: none;}
                .BVDI_QTHeaderGroup .BVDITitle {.less_text_strong;}
                .BVDI_QTSuffixHeaderGroup {}
        .BVDI_QTBodyGroup {}
        .BVDI_QTTags .BVDI_QTBodyGroup {padding: 0;}
        .BVDI_QTTags .BVDI_QTFooterGroup {display: none;}
        .BVDI_QTAttributes .BVDI_QTFooterGroup {display: none;}

        /* BVDI_QTFilterSummary */
        .BVDI_QTFilterSummary {overflow: hidden; zoom: 1;} /* This overflow hidden is important for the hidden inputs we use on filtering. */
        .BVDI_QTFilterSummary:after {.less_box_clearFix;}
            .BVDI_QTHeaderFilterSummary {.less_box_borderLight; .less_box_borderTop; padding: 10px 0;}
                .BVDI_QTHeaderFilterSummary .BVDITitle {.less_text_titleHelper;}
                .BVDI_QTBodyFilterSummary {margin-bottom: 10px; overflow: hidden;}
                    .BVDI_QTSummary {display: inline; margin-right: 10px;}
                    .BVDI_QTSummaryNoContent {.less_text_helper;}
                    .BVDI_QTSummaryOdd {}
                    .BVDI_QTSummaryEven {}
                    .BVDI_QTSummaryFirst {}
                    .BVDI_QTSummaryLast {}
                .BVDI_QTHeaderSummary {}
                    .BVDI_QTPrefixHeaderSummary {.less_text_helper; margin-right: 10px; display: none;}
                    .BVDI_QTSummaryFirst .BVDI_QTPrefixHeaderSummary {display: none;}
                    .BVDI_QTHeaderSummary .BVDITitle {.less_text_strong;}
                .BVDI_QTSummary .BVDI_QTFilterList {display: inline;}
                    .BVDI_QTSummary .BVDI_QTFilter {display: inline; padding: 1px 0; vertical-align: top; zoom: 1;}
            .BVDI_QTFooterFilterSummary {.less_text_iconClear; line-height: 1.2em;}
                .BVDI_QTFooterFilterSummary .BVDI_QTLinkRemoveAll {.less_text_linkAction;}
                .BVDI_QTFooterFilterSummary .BVDI_QTLinkRemoveAll:visited {.less_text_linkActionVisited;}
                .BVDI_QTFooterFilterSummary .BVDI_QTLinkRemoveAll:hover {.less_text_linkActionHover;}
                .BVDI_QTFooterFilterSummary .BVDI_QTLinkRemoveAll:focus {.less_text_linkActionFocus;}
                .BVDI_QTFooterFilterSummary .BVDI_QTLinkRemoveAll:active {.less_text_linkActionActive;}
                .BVDI_QTFooterFilterSummary .BVDI_QTLinkRemoveAll, .BVDI_QTFooterFilterSummary .BVDI_QTLinkRemoveAll:visited, .BVDI_QTFooterFilterSummary .BVDI_QTLinkRemoveAll:hover, .BVDI_QTFooterFilterSummary .BVDI_QTLinkRemoveAll:focus, .BVDI_QTFooterFilterSummary .BVDI_QTLinkRemoveAll:active {color: @less_text_colorAlertBad;}
        .BVDI_QTFilterSummary .BVDI_QTFilterDisabled {display: none;}

        /* Filter Styles */
        ul.BVDI_QTFilterList {margin: 0; padding: 0; list-style-type: none; zoom: 1;}
            li.BVDI_QTFilter {overflow: hidden;}
            .BVDI_QTFilterList li.BVDI_QTFilter {padding-right: 10px;}
                .BVDI_QTFilterList li.BVDI_QTFilter .BVDIValue em {.less_text_helper; padding-left: 5px;} /* locale has helper text */
            .BVDI_QTFilterOdd {}
            .BVDI_QTFilterEven {}
            .BVDI_QTFilterFirst {}
            .BVDI_QTFilterLast {}
            .BVDI_QTPrefixFilter {}
            .BVDI_QTSuffixFilter {display: none;}
            .BVDI_QTFilterLast .BVDI_QTSuffixFilter {display: none;}
            .BVDI_QTFilterLastSelected .BVDI_QTSuffixFilter {display: none;}
            .BVDI_QTGroup .BVDI_QTSuffixFilter  {display: none;}
            /* Filter checkbox links */
            /* padding-left must be set to 0 because the padding is applied to the label (inside the filter) instead. */
            .BVDI_QTFilterListCheckboxes .BVDI_QTFilter label {.less_text_checkFilterOff;}
            .BVDI_QTFilterListCheckboxes .BVDI_QTFilterChecked label {.less_text_checkFilterOn;}
            .BVDI_QTFilterListCheckboxes .BVDI_QTFilterDisabled label {.less_text_checkFilterDisabled;}
            .BVDI_QTFilterListCheckboxes .BVDI_QTFilterHover label {.less_text_checkFilterOffHover;}
            .BVDI_QTFilterListCheckboxes .BVDI_QTFilterCheckedHover label {.less_text_checkFilterOnHover;}
            .BVDI_QTFilterListCheckboxes .BVDI_QTFilterFocus label {background-color: @less_box_colorPrimaryFadest;}
            .BVDI_QTFilterListCheckboxes .BVDI_QTFilterFocus {background-color: @less_box_colorPrimaryFadest; outline: 1px dotted @less_box_borderColorMed;}
            .BVDI_QTFilterListRadios .BVDI_QTFilter label {.less_text_radioFilterOff;}
            .BVDI_QTFilterListRadios .BVDI_QTFilterChecked label {.less_text_radioFilterOn;}
            .BVDI_QTFilterListRadios .BVDI_QTFilterDisabled label {.less_text_radioFilterDisabled;}
            .BVDI_QTFilterListRadios .BVDI_QTFilterHover label {.less_text_radioFilterOffHover;}
            .BVDI_QTFilterListRadios .BVDI_QTFilterCheckedHover label {.less_text_radioFilterOnHover;}
            .BVDI_QTFilterListRadios .BVDI_QTFilterFocus {background-color: @less_box_colorPrimaryFadest; outline: 1px dotted @less_box_borderColorMed;}
                .BVDI_QTFilter label {line-height: normal; display: inline-block; *display: inline; zoom: 1;} /* HACK: IE6 IE7 [dbeberniss] display: inline; property necessary due to incomplete support of display: inline-block; - used to keep in line with checkbox inputs */
                .BVDI_QTFilter label:hover {cursor: pointer;}
                .BVDI_QTFilterDisabled label {}
                .BVDI_QTFilterDisabled label:hover {cursor: auto;}
                .BVDI_QTFilter .BVDIValue {}
                .BVDI_QTFilter .BVDISuffix {.less_text_helper; margin-left: 1ex;}
                .BVDI_QTFilter .BVDINumber {.less_text_helper; margin-left: 1ex;}
                .BVDI_QTFilter input {margin-left: -40px; float: left;}
            .BVDI_QTLevel0 label {color: @less_text_colorLow;} /* Disabled filters */
            .BVDI_QTFilter label {}
            .BVDI_QTLevel1 .BVDIValue {}
            .BVDI_QTLevel2 .BVDIValue {}
            .BVDI_QTLevel3 .BVDIValue {}
            .BVDI_QTLevel4 .BVDIValue {}
            .BVDI_QTLevel5 .BVDIValue {}
            .BVDI_QTLevel6 .BVDIValue {}
            .BVDI_QTLevel7 .BVDIValue {}
            .BVDI_QTLevel8 .BVDIValue {}
            .BVDI_QTLevel9 .BVDIValue {}
            .BVDI_QTLevel10 .BVDIValue {}
            .BVDI_QTLevel11 .BVDIValue {}
            .BVDI_QTLevel12 .BVDIValue {}
            .BVDI_QTLevel13 .BVDIValue {}
            .BVDI_QTLevel14 .BVDIValue {}
            .BVDI_QTLevel15 .BVDIValue {}
                .BVDI_QTSuffixFilter {.less_text_helper; margin: 0 0 0 5px;}
                .BVDI_QTFilterFilteringDisabled .BVDI_QTFilter .BVDIValue {color: @less_text_colorLow;}
                .BVDI_QTFilterFilteringDisabled .BVDI_QTFilter label:hover {cursor: wait;}

        .BVDI_QTFooterGroup {}
        .BVDI_QTFooterGroup {overflow: hidden; zoom: 1; padding: 0 5px; position: relative; top: -2px;}
        /* Toggle */
        .BVDI_QT .BVDILinkToggle {}
            .BVDI_QTFooterGroup .BVDILinkToggle {.less_box_widget; .less_box_borderLight; border-top-color: @less_box_colorBase; display: block; padding: 0 2px 2px; width: 80px; text-align: center;}
            .BVDI_QTFooterGroup .BVDILinkToggle {.less_text_linkAction;}
            .BVDI_QTFooterGroup .BVDILinkToggle:visited {.less_text_linkActionVisited;}
            .BVDI_QTFooterGroup .BVDILinkToggle:hover {.less_text_linkActionHover; outline: none;}
            .BVDI_QTFooterGroup .BVDILinkToggle:focus {.less_text_linkActionFocus; outline: none;}
            .BVDI_QTFooterGroup .BVDILinkToggle:active {.less_text_linkActionActive; outline: none;}

            /* These more/fewer links exist in the tag filtering footers - the span must be targeted because the link already has a background in default. */
            .BVDI_QTGroupCollapsed .BVDI_QTFooterGroup .BVDILinkToggle .BVDILinkSpan {.less_text_iconDown;}
            .BVDI_QTGroupExpanded .BVDI_QTFooterGroup .BVDILinkToggle .BVDILinkSpan {.less_text_iconUp;}

        .BVDI_QT .BVDIToggleArea {width: 180px;}
            .BVDI_QTAttributes .BVDIToggleArea {.less_box_fakeSelectOpen; position: absolute; padding: 10px; z-index: 2;}
    .BVDI_QTFooter {}
    .BVDI_QTSpacerInside {display: block; clear: left;}

    /* Filtering customizations for Tags (i.e., non-popin filters) */
    .BVDI_QTBodyTags .BVDI_QTGroupCollapsed .BVDITitle .BVDILinkToggle {}
    .BVDI_QTBodyTags .BVDI_QTGroupExpanded .BVDITitle .BVDILinkToggle {}
    .BVDI_QTTags .BVDIToggleArea {display: none !important;}
        .BVDI_QTTags ul.BVDI_QTFilterList {display: inline;}
        .BVDI_QTTags li.BVDI_QTFilter {display: block; float: left; margin-right: 10px; white-space: nowrap; zoom: 1;}
        .BVDI_QTTags li.BVDI_QTFilter label {display: block; float: left; white-space: nowrap;}

    /* Filtering customizations for Attributes (i.e., popin filters) */
    /* --- OPTION 1: MINIMALIST --- */
        /* @group Minimalist */
        .BVDI_QTAttributes .BVDI_QTGroupExpanded .BVDITitle .BVDILinkToggle {.less_text_iconExpanded; display: inline-block;}
        .BVDI_QTAttributes .BVDI_QTGroupCollapsed .BVDITitle .BVDILinkToggle {.less_text_iconCollapsed; display: inline-block;}
        /* @end */
    /* --- END OPTION 1: MINIMALIST --- */
