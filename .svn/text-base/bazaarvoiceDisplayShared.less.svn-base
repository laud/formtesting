                                    .BVDI_PSHistogramBar {.less_box_borderMed; background-color: @less_box_colorBase; float: left; margin: 0 5px; width: 100px; height: 8px; font-size: 6px;} /* Don't lessify This font size is just for layout - do not disturb */
                                    .BVDI_PSHistogramBarRowZero .BVDI_PSHistogramBar {}
                                        .BVDI_PSHistogramFullBar {background-color: @less_box_btnColorPrimaryMed; float: left; height: 8px;}
                                    .BVDI_PSHistAbsLabel {float: left; display: none;}
                                    .BVDI_PSHistPercLabel {float: left;}
                .BVDI_PSBodyProductSpecificQA {}
            .BVDI_PSContainerCrossLinking {.less_text_meta; margin-left: 45%;}
                .BVDI_PSHeaderCrossLinking {.less_text_strong; margin-bottom: 10px;}
                .BVDI_PSBodyCrossLinking {margin-bottom: 5px; padding: 5px;}
                    .BVDI_PSSectionCrossLinking {}
