                    .BVDI_PSSectionHistogram {}
                        .BVDI_PSHistogram {}
                            .BVDI_PSHistogramTitle {display: none;}
                                .BVDI_PSHistogramTitleText {}
                                .BVDI_PSHistogramTitle .BVDI_PSCount {}
                                    .BVDI_PSHistogramTitle .BVDI_PSNumber {}
                            .BVDI_PSHistogramContent {}
