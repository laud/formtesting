                        .BVDI_PSSectionCrossLinkingQA .BVDI_PSLabelValuePair:after {.less_box_clearFix;}
                            .BVDI_PSQuestionAnswersCount {.less_text_helper; float: right; margin-left: 10px; margin-bottom: 5px;}
                            .BVDI_PSQuestion {margin-bottom: 5px;}
                                .BVDI_PSQuestionPrefix {.less_text_strong;}
                                .BVDI_PSQuestionText {}
                            .BVDI_PSQuestion {}
            .BVDI_PSContainerCredibility {clear: both; margin-bottom: 0;}
                .BVDI_PSHeaderCredibility {}
                .BVDI_PSBodyCredibilityVoting {}
                        .BVDI_PSSectionFeedback, .BVDI_PSSectionFeedback div {float: left; height: 20px; line-height: 20px; white-space: nowrap;}
                        .BVDI_PSSectionFeedback {.less_box_feedbackBackgroundLeft; .less_box_feedbackNormalLeft; .less_box_opacity70; .less_text_strong; .less_text_uppercase; margin-right: 10px; padding-left: 5px;}
                            .BVDI_PSHeaderFeedback {display: none;}
                            .BVDI_PSBodyFeedback {.less_box_feedbackBackgroundRight; .less_box_feedbackNormalRight; padding-right: 10px;}
                                .BVDI_PSBodyFeedback .BVDI_PSLabelValuePair {}
                                    .BVDI_PSBodyFeedback .BVDI_PSLabel {display: none;}
                                    .BVDI_PSBodyFeedback .BVDI_PSValue {.less_text_feedbackIconDisabled; .less_text_titleHelper; margin-right: 10px;}
                                .BVDI_PSLabelValuePairYes {}
                                    .BVDI_PSLabelValuePairYes .BVDI_PSValue {.less_text_feedbackIconGood;}
                                    .BVDI_PSLabelValuePairNo .BVDI_PSValue {.less_text_feedbackIconBad;}
                    .BVDI_PSSectionBestAnswer {.less_text_helper; overflow: hidden; zoom: 1; vertical-align: baseline; line-height: 20px;}
                        .BVDI_PSHeaderBestAnswer {display: block; float: left; margin: 0 10px;}
                        .BVDI_PSBodyBestAnswer {}
                        .BVDI_PSBodyBestAnswer .BVDI_PSLabelValuePair {float: left; margin-right: 10px;}
                            .BVDI_PSBodyBestAnswer .BVDI_PSLabel {display: inline;}
                            .BVDI_PSBodyBestAnswer .BVDI_PSValue {.less_box_reset; .less_text_titleHelper; display: inline;}
                    .BVDI_PSSectionFeatured {overflow: hidden; zoom: 1; vertical-align: baseline; line-height: 20px;}
                        .BVDI_PSHeaderFeatured {.less_text_normal; display: block; float: left; margin-right: 10px; margin-top: 5px;}
                        .BVDI_PSBodyFeatured {}
                        .BVDI_PSBodyFeatured .BVDI_PSLabelValuePair {float: left; margin-right: 10px;}
                            .BVDI_PSBodyFeatured .BVDI_PSLabel {display: inline;}
                            .BVDI_PSBodyFeatured .BVDI_PSValue {.less_text_meta; .less_text_size4; .less_text_strong; display: inline;}
                .BVDI_PSBodyCredibilityBadges {margin-bottom: 0;}
                    .BVDI_PSBodyCredibilityBadges .BVDI_PSSectionBadges {clear: both;}
                        .BVDI_PSBodyCredibilityBadges .BVDI_BABadges {}
        .BVDI_PSFooter {.less_box_toolbar; .less_box_borderTop; padding: 5px; text-align: right; clear: left; }
