/* Shared photo (BVDI_PH) */
.BVDI_PH {.less_box_reset; display: inline-block; vertical-align: top; width: auto; zoom: 1; *display: inline;} /* HACK: IE6 IE7 [dbeberniss] display: inline; property necessary due to incomplete support of display: inline-block; - used to keep photo and video on one line */
    .BVDI_PHPhotoSectionLabel, .BVDI_PHPhotoSectionHint {display: none;}
    .BVDI_PHPhoto {.less_box_reset; margin: 0 5px 5px 0; display: inline-block; *display: inline; zoom: 1;} /* HACK: IE6 IE7 [dbeberniss] display: inline; property necessary due to incomplete support of display: inline-block; - used to create single row of thumbnails */
    .BVDI_PHPhotoLine {.less_box_reset; padding: 0; display: inline;}

    /* Photo Hero */
    .BVDI_PHPhotoHeroContainer {text-align: center;}
    .BVDI_PHPhotoHeroCaption {padding: 5px;}
    .BVDI_PHPhotoHeroContainer .BVDI_PHPhotoLine {padding: 5px 0; overflow: hidden; zoom: 1;}
        .BVDI_PHPhotoHeroContainer .BVDI_PHPhoto {float: left; margin: 5px 5px 5px 0;}

    /* Photo Thumbs */
    .BVDI_PHPhotoSectionThumbnail {.less_box_opacity70; .less_box_borderLight; .less_box_borderLarge; display: block; cursor: pointer; overflow: hidden; height: 50px; width: 50px;}
    .BVDI_PHPhotoSectionThumbnail img {width: 100px; margin-left: -25px; height: auto;}
    .BVDI_PHPhotoSectionThumbnail:hover {.less_box_opacity100; .less_box_borderMed; .less_box_borderLarge;}

    /* Photo Popin */
.BVDI_PHPhotoPopup {.less_box_popin; .less_text_body; .less_text_helper; padding: 10px; cursor: pointer; position: absolute; z-index: 1000; zoom: 1; *width: 500px;} /* co photo popup needs IE width */
    .BVDI_PHPhotoPopupHint {.less_text_iconClose; .less_text_iconOnly; float: right; /*position: absolute; top: 5px; right: 13px;*/} /* can have float IF popup has fixed width in IE */
    .BVDI_PHPhotoPopupImage {text-align: center;}
    .BVDI_PHPhotoPopupImage img {*width: 300px; height: auto; -ms-interpolation-mode: bicubic; zoom: 1;} /* Flexible widths work on all browsers except early IE, so we're forcing resize on IE6 & 7 */
    .BVDI_PHPhotoPopupCaption {text-align: center; display: block; margin: 3px auto; width: 400px !important;} /* The important is required because the javascript attempts to auto-size the popup. */


/* Shared video */
.BVDI_VI {display: inline-block; vertical-align: top; zoom: 1; *display: inline;} /* HACK: IE6 IE7 [dbeberniss] display: inline; property necessary due to incomplete support of display: inline-block; - used to keep photo and video on one line */
.BVDI_VIVideoSectionHeader {display: none;}
    .BVDI_VIVideoSectionLabel {.less_text_strong;}
    .BVDI_VIVideoSectionHint {.less_text_helper;}
    .BVDI_VIInternalVideoPreview {.less_box_borderLight; padding: 5px;}

    .BVVIVideoPopup {.less_box_popin; .less_text_helper; position: absolute; z-index: 1004; *width: 360px;} /* Fixed width required for IE 6-7, should be in sync with site.embeddedVideoWidth + padding */
        .BVVIVideoPopup a img {border: 0;}
        .BVVIVideoPopup .BVDIInside {padding: 20px; text-align: center;}
        .BVVIInternalVideoPreview .BVDIInside {.less_text_helper; text-align: center;}
        .BVVIVideoPopupClose {overflow: hidden; zoom: 1; margin-bottom: 5px; text-align: right; white-space: nowrap;}
        .BVVIVideoPopup .BVVIVideoPlayer {text-align: center;}
        .BVVIVideoPopup .BVVIVideoSingleCaption {.less_text_helper; display: block; width: 320px; margin-top: 5px; text-align: center;}
        .BVVIVideoPopup .BVVIVideo embed {.less_box_borderNo; margin: 0 auto;}

    .BVVIInternalVideoPreview {clear: both; width: 280px; height: 210px;}


    /* Video Thumbnail with Play Button Icon */
    .BVDI_VIVideoThumbnail, .BVVIVideoThumbnail {.less_box_borderLight; .less_box_borderLarge; .less_box_opacity70; position: relative; cursor: pointer; display: inline-block; height: 50px; width: 50px; margin: 0 5px 5px 0; *display: inline; zoom: 1;} /* HACK: IE6 IE7 [dbeberniss] display: inline; property necessary due to incomplete support of display: inline-block; - used to create single row of thumbnails */
    .BVDI_VIVideoThumbnail:hover, .BVVIVideoThumbnail:hover {.less_box_borderMed; .less_box_borderLarge; .less_box_opacity100;}
        .BVVIVideoThumbnail img, .BVVIVideoThumbnail img {height: 100%; width: 100%;}

        .BVVIVideoIcon {.less_box_reset; width: 25px; margin-left: -12px; height: 25px; margin-top: -12px; position: absolute; left: 50%; top: 50%; z-index: 1;}
        .BVVIVideoIcon img {height: 100%; width: 100%; margin-left: 0;}
            .BVVIVideoIcon img, .BVVIVideoIcon img:hover {background-color: transparent !important;}


