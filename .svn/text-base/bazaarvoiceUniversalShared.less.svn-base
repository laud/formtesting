/* =OVERALL JQUERY RATING CSS
    ------------------------------------------------------
    jQuery.Rating Plugin v2.5 (with bazaarvoice patch) CSS - http://www.fyneworks.com/jquery/star-rating/
    These styles came from the jQuery distribution. Override them below, but do not restructure this area so it can be compared with default. */
    .BVField div.rating {cursor: pointer; margin: 2em; clear: both; display: block;}
    .BVField div.rating:after {content: '.'; display: block; height: 20px; width: 0; clear: both; visibility: hidden;}
    .BVField div.star a {zoom: 1; outline: none;}
    .BVField div.cancel, .BVField div.star {float: left; width: 20px; height: 20px; text-indent: -9999em; cursor: pointer; display: block; background: transparent; overflow: hidden; zoom: 1;}
    .BVField div.cancel, .BVField div.cancel a {background: url(ratingStars_cancel.gif) no-repeat 0 -20px;}
    .BVField div.star, .BVField div.star a {background: url(ratingStars_star.gif) no-repeat 0 0;}
    .BVField div.cancel a, .BVField div.star a {display: block; width: 20px; height: 20px; background-position: 0 0; outline-width: 0;}
    .BVField div.star_on a {background-position:0 -40px;}
    .BVField div.star_hover a, .BVField div.star_live a:hover {background-position: 0 -20px;}
    /* Read Only CSS */
    .BVField div.star_readonly a, .BVField div.star_readonly a:hover {cursor: default;}
    /* Partial Star CSS */
    .BVField div.star {background: transparent; overflow: hidden;}
    /* END jQuery.Rating Plugin CSS */



/* =SECONDARY JQUERY RATING CSS
    ------------------------------------------------------
    uncomment to use different graphics (ratingStars_secondary.gif) for secondary ratings */
 .BVFieldRatingsGroup div.rating {cursor: pointer; margin: 2em; clear: both; display: block;}
     .BVFieldRatingsGroup div.rating:after {content: '.'; display: block; height: 27px; width: 0; clear: both; visibility: hidden;}
     .BVFieldRatingsGroup div.star a {zoom: 1;}
     .BVFieldRatingsGroup div.cancel, .BVFieldRatingsGroup div.star {float: left; width: 28px; height: 7px; padding-top: 5px; text-indent: -999em; cursor: pointer; display: block; background: transparent; overflow: hidden; zoom: 1;}
     .BVFieldRatingsGroup div.cancel, .BVFieldRatingsGroup div.cancel a {background: url(ratingStars_cancel.gif) no-repeat 0 -24px;}
     .BVFieldRatingsGroup div.star, .BVFieldRatingsGroup div.star a {background: url(ratingStars_secondary.gif) no-repeat 0 0;}
     .BVFieldRatingsGroup div.cancel a, .BVFieldRatingsGroup div.star a {display: block; width: 28px; height: 7px; background-position: 0 0;}
     .BVFieldRatingsGroup div.star_on a {background-position: 0 -7px;}
     .BVFieldRatingsGroup div.star_hover a, .BVFieldRatingsGroup div.star_live a:hover {background-position: 0 -14px;}
     /* Read Only CSS */
     .BVFieldRatingsGroup div.star_readonly a, .BVFieldRatingsGroup div.star_readonly a:hover {cursor: default;}
     /* Partial Star CSS */
     .BVFieldRatingsGroup div.star {background: transparent; overflow: hidden;}
     /* END jQuery.Rating Plugin CSS */

     /* cancel button */
     .BVFieldRatingsGroup div.cancel {display: none;}



/* =SLIDERS RATING JQUERY CSS
    ------------------------------------------------------ */

    /* jQuery ui-slider CSS 1.5.2 */
    /* BAZAARVOICE NOTE: THESE STYLES CAME FROM THE JQUERY DISTRIBUTION. OVERRIDE THEM BELOW, DO NOT RESTRUCTURE THIS AREA SO IT CAN BE COMPARED WITH DEFAULT. */
    .BVSliderField .ui-slider {width: 278px; height: 18px; position: relative; background-repeat: no-repeat; background-position: center center;}
    .BVSliderField .ui-slider-handle {position: absolute; z-index: 1; height: 18px; width: 18px; top: 0; left: 0; background-image: url(slider_handle.gif); cursor: pointer;}
	.BVSliderField .ui-state-active {background: url(slider_handle_active.gif);}
    /* Uncomment the following line to enable dotted frame around active slider */
    /*.BVRRWidget .ui-slider-handle-active {outline: 1px dotted black;}*/
    .BVSliderField .ui-slider-disabled .ui-slider-handle {opacity: 0.5; filter: alpha(opacity=50);}
    .BVSliderField .ui-slider-range {position: absolute; background: #50a029; opacity: 0.3; filter: alpha(opacity=30); width: 100%; height: 100%;}
    /* Default slider backgrounds */
    .BVSliderField .ui-slider {background-image: url(slider_background.gif);}
    /* END ui-slider CSS */

