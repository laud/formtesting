    .BVFieldPreview .BVButton:hover {.less_box_btnHighHover; .less_text_btnHighHover;}

    /* submit button (button) */
    .BVFieldSubmit .BVButton {.less_box_btnHigh; .less_text_btnHigh;}
    .BVFieldSubmit .BVButton:hover {.less_box_btnHighHover; .less_text_btnHighHover;}

