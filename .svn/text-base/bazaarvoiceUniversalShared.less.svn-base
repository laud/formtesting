    .BVLinkTranslate a, .BVLinkTranslate a:visited {.less_box_btnHigh; .less_text_btnHigh;}
    .BVLinkTranslate a:hover, .BVLinkTranslate a:focus, .BVLinkTranslate a:active {.less_box_btnHighHover; .less_text_btnHighHover;}

