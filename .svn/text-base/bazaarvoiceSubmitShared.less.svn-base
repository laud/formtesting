/* @group SubmissionPagination */ 
/* PAGINATION */
/* There's no reason that SU-prefixed pagination won't appear lots of other places eventually.
   But right now pagination with the SU prefix only appears on PL and GRS. */
/* Pagination Styles (Submission) */
.BVSUPager {.less_box_pagination; margin: 0; padding: 5px;}
.BVSUSimplePager {position: relative;}
    .BVSUSimplePagerColumn {}
    .BVSUCurrentPage {text-align: center;}
    .BVSUSimplePager .BVSUPreviousPage {position: absolute; top: 5px;}
    .BVSUSimplePager .BVSUNextPage {position: absolute; right: 5px; top: 5px;}
.BVSUPageBasedPager {.less_box_pagination; .less_text_pagination; text-align: center;}
    .BVSUPageLink {display:inline; padding: 0 2px;}
    .BVSUPageNumber {}
    .BVSUPageNumber a {.less_text_linkAction;}
    .BVSUPageNumber a:visited {.less_text_linkActionVisited;}
    .BVSUPageNumber a:hover {.less_text_linkActionHover;}
    .BVSUPageNumber a:focus {.less_text_linkActionFocus;}
    .BVSUPageNumber a:active {.less_text_linkActionActive;}
    .BVSUPageBasedPager .BVSUPreviousPage {}
    .BVSUPageBasedPager .BVSUNextPage {}
    .BVSUSelectedPageNumber {.less_box_paginationSelected; .less_text_paginationSelected;}

    .BVSUPagerArrows {display: none;}

