    .BVSU_SearchButtonContainer .BVSUInputButtonSearch:hover {.less_box_btnLowHover; .less_text_btnLowHover;}

/* remove button */
.BVSULinkRemove {.less_text_iconOnly; .less_text_iconClose; float: right; display: block; overflow: hidden; padding: 0;}
    .BVSULinkRemove:hover {.less_box_opacity70;}

