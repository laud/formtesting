/*
    Some basic submission sections are also used on inline submission forms as well as standalone forms.
    Refer to bazaarvoiceDisplayShared for the remaining sections.
*/

.BVSUModalContent, .BVSUPage {.less_text_body; text-align: left;}
/* redeclaring link colors as it doesn't appropriately inherit from core products */
.BVSUModalContent a {.less_text_link;}
.BVSUModalContent a:visited {.less_text_linkVisited;}
.BVSUModalContent a:hover {.less_text_linkHover;}
.BVSUModalContent a:focus {.less_text_linkFocus;}
.BVSUModalContent a:active {.less_text_linkActive;}

/* Button Styles. */
/* Change me to match the client sites! Be sure to match these styles in the other CSS files also! */
/* If you want progressive enhancements, find them in progressive */

    /* General button and input styles */
    .BVSUButton, .BVSUModule .BVSUButton {.less_box_btnLow; .less_text_btnLow; cursor: pointer;}
    .BVSUButton:hover, .BVSUModule .BVSUButton:hover {.less_box_btnLowHover; .less_text_btnLowHover;}

    /* Product Linking Search Button */
    .BVSUSearchButton button {display: inline-block; *display: inline;} /* HACK: IE6 IE7 [dbeberniss] display: inline; property necessary due to incomplete support of display: inline-block; */
        .BVSUSearchButton button:hover {}

    /* Product Linking general buttons */
    .BVSUModalContent button {margin: 0 10px 0 0; vertical-align: top; padding: 5px 10px; cursor: pointer;}
    .BVSUModalContent button {.less_box_btnLow; .less_text_btnLow;}
    .BVSUModalContent button:hover {.less_box_btnLowHover; .less_text_btnLowHover;}

    /* Product Linking Add a Product button */
    button.BVSUInputButtonAdd {margin: 0 0 0 10px; vertical-align: top; padding: 1px 7px; cursor: pointer; *padding: 0;} /* HACK: IE6 IE7 [shippert] Add and Added buttons have much larger spacing in IE6/7, removing padding to keep consistent with other browsers */
    button.BVSUInputButtonAdd {.less_box_btnHigh; .less_text_btnHigh;}
    button.BVSUInputButtonAdd:hover {.less_box_btnHighHover; .less_text_btnHighHover;}

    /* Product Linking Added Product non-button */
    button.BVSUInputButtonAdded {margin: 0 0 0 10px; padding: 1px 7px; cursor: default; *padding: 0;} /* HACK: IE6 IE7 [shippert] Add and Added buttons have much larger spacing in IE6/7, removing padding to keep consistent with other browsers */
    button.BVSUInputButtonAdded, button.BVSUInputButtonAdded:hover {.less_box_btnDisabled; .less_text_btnDisabled; .less_box_opacity100;}

    /* Product Linking Search buttons */
    .BVSUModalContent .BVSUInputButtonSearch {margin: 0 10px 0 0; vertical-align: top; padding: 5px 10px; *padding: 3px 0 0;} /* HACK: IE6 IE7 [dbeberniss] Reduce padding on all sides to match styling of other HTML buttons in this window - set height with top padding to match the browse link-as-button beside it */
    .BVSUModalContent .BVSUInputButtonSearch {.less_box_btnLow; .less_text_btnLow;}
    .BVSUModalContent .BVSUInputButtonSearch:hover {.less_box_btnLowHover; .less_text_btnLowHover;}

    /* Product Linking Browse button */
    .BVSU_Search .BVSULinkBrowseAllProducts, .BVSU_Search .BVSULinkBrowseAllProducts:visited {margin: 0 10px 0 0; display: inline-block; vertical-align: top; padding: 5px 10px; *display: inline;} /* HACK: IE6 IE7 [dbeberniss] display: inline; necessary due to incomplete support of display: inline-block; */
    .BVSU_Search .BVSULinkBrowseAllProducts, .BVSU_Search .BVSULinkBrowseAllProducts:visited {.less_box_btnLow; .less_text_btnLow;}
    .BVSU_Search .BVSULinkBrowseAllProducts:hover, .BVSU_Search .BVSULinkBrowseAllProducts:focus, .BVSU_Search .BVSULinkBrowseAllProducts:active {.less_box_btnLowHover; .less_text_btnLowHover;}

    /* Product Linking Go Back/Done button*/
    .BVSULinkGoBack, .BVSUModalContent .BVSULinkGoBack {margin: 0 10px 0 0; vertical-align: top; padding: 5px 10px;}
    .BVSULinkGoBack, .BVSUModalContent .BVSULinkGoBack, .BVSULinkGoBack:visited, .BVSUModalContent .BVSULinkGoBack:visited {.less_box_btnHigh; .less_text_btnHigh;}
    .BVSULinkGoBack:hover, .BVSUModalContent .BVSULinkGoBack:hover, .BVSULinkGoBack:focus, .BVSUModalContent .BVSULinkGoBack:focus, .BVSULinkGoBack:active, .BVSUModalContent .BVSULinkGoBack:active {.less_box_btnHighHover; .less_text_btnHighHover;}
        .BVSULinkGoBack span, .BVSUModalContent .BVSULinkGoBack span {color: inherit;}

    .BVSU_SearchButtonContainer .BVSUInputButtonSearch {padding: 3px 10px; *padding: 0;} /* HACK: IE6 IE7 [shippert] Add and Added buttons have much larger spacing in IE6/7, removing padding to keep consistent with other browsers */
